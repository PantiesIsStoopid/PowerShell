{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "rprompt",
      "segments": [
        {
          "foreground": "#c6d0f5",
          "properties": {
            "time_format": "3:04PM"
          },
          "style": "plain",
          "template": " {{ .CurrentDate | date .Format }} ",
          "type": "time"
        }
      ]
    },
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#8caaee",
          "foreground": "#c6d0f5",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "{{ .UserName }} ",
          "type": "session"
        },
        {
          "background": "#ca9ee6",
          "foreground": "#c6d0f5",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "style": "folder"
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#e5c890",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#e5c890{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f2d5cf{{ end }}",
            "{{ if gt .Ahead 0 }}#ca9ee6{{ end }}",
            "{{ if gt .Behind 0 }}#ca9ee6{{ end }}"
          ],
          "foreground": "#c6d0f5",
          "leading_diamond": "<transparent,background>\uE0B0</>",
          "trailing_diamond": "\ue0b0",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true
          },
          "style": "diamond",
          "template": " {{ .HEAD }}{{ if .Staging.Changed }}<#e5c890> \uf046 {{ .Staging.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "#81c8be",
          "foreground": "#c6d0f5",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        },
        {
          "background": "#f2d5cf",
          "foreground": "#c6d0f5",
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "properties": {
            "style": "dallas",
            "threshold": 0
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }}s ",
          "trailing_diamond": "\ue0b0",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
